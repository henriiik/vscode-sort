{
  "name": "vscode-sort",
  "displayName": "Sort",
  "description": "Sort lines or words with a command.",
  "version": "0.0.1",
  "publisher": "henriiik",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sort"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sort",
        "title": "Sort Selection"
      }
    ],
    "keybindings": [
      {
        "key": "shift+alt+s",
        "command": "extension.sort",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Sort configuration",
      "type": "object",
      "properties": {
        "sort.locale": {
          "type": "string",
          "default": "en",
          "description": "The locale to use in sorting comparison."
        },
        "sort.ignore-case": {
          "type": "boolean",
          "default": false,
          "description": "Wether or not the sort should ignore case."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "tslint": "^3.1.1",
    "typescript": "^1.7.3",
    "vscode": "0.10.x"
  }
}